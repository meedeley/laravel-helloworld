{
  "name": "meedeley/hello-world",
  "description": "Just a simple library to generate greetings or messages in Laravel using Artisan command.",
  "type": "library",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Nichola Saputra",
      "email": "nicholasaputra77@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0"
  },
  "autoload": {
    "psr-4": {
      "Meedeley\\HelloWorld\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Meedeley\\HelloWorld\\HelloWorldServiceProvider"
      ],
      "aliases": {
        "HelloWorld": "Meedeley\\HelloWorld\\Facades\\HelloWorld"
      }
    }
  }
}
